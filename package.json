{
  "name": "naamapi",
  "version": "0.0.1",
  "description": "Serverless API for the Naamat festival application",
  "main": "handler.js",
  "scripts": {
    "test": "jest ./tests"
  },
  "dependencies": {
    "aws-sdk": "^2.409.0",
